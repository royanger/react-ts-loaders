{
   "name": "react-ts-loaders",
   "version": "0.1.0",
   "author": "Roy Anger",
   "license": "MIT",
   "private": false,
   "main": "dist/index.js",
   "module": "dist/index.js",
   "files": [
      "dist",
      "README.md"
   ],
   "dependencies": {
      "@testing-library/jest-dom": "^5.11.4",
      "@testing-library/react": "^11.1.0",
      "@testing-library/user-event": "^12.1.10",
      "@types/jest": "^26.0.15",
      "@types/node": "^12.0.0",
      "@types/react": "^17.0.0",
      "@types/react-dom": "^17.0.0",
      "react": "^17.0.2",
      "react-dom": "^17.0.2",
      "react-scripts": "4.0.3",
      "typescript": "^4.1.2",
      "web-vitals": "^1.0.1"
   },
   "scripts": {
      "start": "react-scripts start",
      "prebuild": "rimraf dist",
      "build": "babel ./src/lib --out-dir ./dist --extensions .ts,.tsx",
      "compile-styles": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 src/lib/index.scss dist/index.css",
      "format": "prettier-eslint --write \"{,!(node_modules)/**/}*.{js,jsx}\"",
      "test": "react-scripts test",
      "eject": "react-scripts eject"
   },
   "eslintConfig": {
      "extends": [
         "react-app",
         "react-app/jest"
      ]
   },
   "browserslist": {
      "production": [
         ">0.2%",
         "not dead",
         "not op_mini all"
      ],
      "development": [
         "last 1 chrome version",
         "last 1 firefox version",
         "last 1 safari version"
      ]
   },
   "devDependencies": {
      "@babel/cli": "^7.14.3",
      "@babel/core": "^7.14.3",
      "@babel/preset-react": "^7.13.13",
      "@babel/preset-typescript": "^7.13.0",
      "babel-eslint": "^10.1.0",
      "eslint": "^7.27.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-prettier": "^3.4.0",
      "node-sass": "^5.0.0",
      "prettier": "^2.3.0",
      "prettier-eslint-cli": "^5.0.1",
      "rimraf": "^3.0.2"
   }
}
